				mGetString		MACRO prompt,bufferAddress,bufferSize,byteNum

					push	edx
					push	ecx
					push	eax

					mDisplayString	prompt
					mov		edx, bufferAddress
					mov		ecx, bufferSize
					call	ReadString
					mov		[byteNum],eax ; sending back the number of bytes entered 

					pop eax
					pop ecx
					pop	edx
				ENDM

